                    <span class="icon-search search-input-icon" style="z-index: 1"></span>
                    <div class="autocomplete" style="position: relative">
                        <input type="text"
                               data-cat-id="{{ category.id }}"
                               class="form-control search-input-input"
                               placeholder="{% if header_settings.header_search_placeholder %}{{ header_settings.header_search_placeholder }}{% else %}{{ locals.search_placeholder }}{% endif %}"
                        >
                        <div class="autocomplete-items"></div>
                    </div>
                </div>
                {% endif %}

            </div> #}
